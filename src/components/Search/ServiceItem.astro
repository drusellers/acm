---
interface Props {
  headline: string;
  punch: string
}

const { headline, punch } = Astro.props;
---
<div>
    <div class="prose dark:prose-invert text-sm">{headline}</div>
    <div class="prose dark:prose-invert text-2xl font-bold">{punch}</div>
    <div class="prose dark:prose-invert">
        <slot />
    </div>
</div>