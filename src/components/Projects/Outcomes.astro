---
interface Props {
	outcomes: string[];
}
const { outcomes } = Astro.props;
---

<div class="bg-lime-200 dark:bg-lime-900 rounded px-4 py-2 -mx-4">
    <div class="font-bold text-xl text-lime-900">Outcomes</div>
    {outcomes.map(g => {
        return <div class="list-item ml-4">{g}</div>
    })}
</div>